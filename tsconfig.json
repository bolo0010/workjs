{
  "compilerOptions": {
    "target": "es5",
    "jsx": "preserve",
    "allowJs": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true
  },
  "exclude": [
    "client",
    "dist",
    "node_modules"
  ]
}