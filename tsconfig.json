{
  "compilerOptions": {
    "target": "ES2021",
    "jsx": "preserve",
    "moduleResolution": "node",
    "allowJs": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
  },
  "exclude": [
    "client",
    "dist",
    "node_modules",
    "config-overrides.js"
  ],
}